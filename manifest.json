{
	"manifest_version": 2,
	"name": "cMusic Player",
	"description": "Приложение для прослушивания Вашей музыки ВКонтакте",
	"version": "2.0.5",
	"icons": {
		"128": "icons/128.png"
	},
	"app": {
		"background": {
			"scripts": [
				"background.js"
			]
		}
	},
	"commands": {
		"next-track": {
			"description": "next track",
			"global": true,
			"suggested_key": {
				"default": "MediaNextTrack"
			}
		},
		"play-pause": {
			"description": "play/pause",
			"global": true,
			"suggested_key": {
				"default": "MediaPlayPause"
			}
		},
		"previous-track": {
			"description": "previous track",
			"global": true,
			"suggested_key": {
				"default": "MediaPrevTrack"
			}
		},
		"stop": {
			"description": "stop playback",
			"global": true,
			"suggested_key": {
				"default": "MediaStop"
			}
		}
	},
	"permissions": [
		"https://api.vk.com/*",
		"https://oauth.vk.com/*",
		"http://ws.audioscrobbler.com/*",
		"http://fonts.googleapis.com/*",
		"http://userserve-ak.last.fm/*",
		"https://www.google-analytics.com/",
		"fileSystem",
		"storage",
		"webview",
		"notifications",
		"identity",
		"syncFileSystem",
		"unlimitedStorage",
		"<all_urls>"
	],
	"minimum_chrome_version": "36.0.0.0"
}